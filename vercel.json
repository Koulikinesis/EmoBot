{
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.py"
      }
    ],
    "env": {
      "PYTHON_VERSION": "3.12"
    },
    
        "functions": {
          "api/**": {
            "excludeFiles": "{.next,*.cache,node_modules,public,app}/**"
          }
        }
      
  }